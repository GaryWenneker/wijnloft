{
  "name": "wijnloft",
  "version": "2.0.0",
  "description": "A simple listener around Stencil router.",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "unpkg": "dist/ionic/ionic.js",
  "collection:main": "dist/collection/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "run-p start:*",
    "build": "stencil build",
    "start:stencil": "stencil build --dev --watch --serve",
    "sass": "npx node-sass src\\assets\\css\\styles.scss > src\\assets\\css\\styles.css\n",
    "start:watch": "npx node-sass src/ -wo src",
    "bootstrap": "netlify dev:exec node ./scripts/bootstrap-fauna-database.js",
    "zzzzbootstrap": "node ./scripts/bootstrap-fauna-database.js",
    "netlify": "netlify dev"
  },
  "dependencies": {
    "@stencil/router": "^1.0.1",
    "@vaadin/vaadin-grid": "^5.7.7",
    "@vaadin/vaadin-tabs": "^3.2.0",
    "@vaadin/vaadin-text-field": "^2.8.2",
    "faunadb": "^2.13.1"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.5",
    "sass": "^1.29.0",
    "node-sass": "^5.0.0",
    "@stencil/sass": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "netlify-cli": "^2.70.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GaryWenneker/wijnloft"
  },
  "author": "Ion Prodan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GaryWenneker/wijnloft/issues"
  },
  "homepage": "https://github.com/GaryWenneker/wijnloft"
}
