{
  "name": "wijnloft",
  "version": "2.0.0",
  "description": "A simple listener around Stencil router.",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "unpkg": "dist/ionic/ionic.js",
  "collection:main": "dist/collection/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "run-p start:*",
    "build": "stencil build",
    "start:stencil": "stencil build --dev --watch --serve",
    "sass": "npx node-sass src\\assets\\css\\styles.scss > src\\assets\\css\\styles.css\n",
    "start:watch": "npx node-sass src/ -wo src"
  },
  "dependencies": {
    "@stencil/router": "^1.0.1"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.5",
    "sass": "^1.29.0",
    "node-sass": "^5.0.0",
    "@stencil/sass": "^1.4.1",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GaryWenneker/wijnloft"
  },
